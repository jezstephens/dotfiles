# vim: ft=sh

"amixer set Master 5%+ -q"
  XF86AudioRaiseVolume

"amixer set Master 5%- -q"
  XF86AudioLowerVolume

"st -e tmux"
  mod4 + Return

"st -e vim"
  mod4 + u

"st -e ranger"
  mod4 + o

#"chromium"
"firefox-developer"
  mod4 + i

#"chromium --incognito"
"firefox-developer --private-window"
  mod4 + shift + i

"rofi -show run -lines 5 -columns 3"
  mod4 + p

"rofi -show ssh"
  mod4 + comma

"i3-dmenu-desktop --dmenu='rofi -dmenu -p run: -lines 5 -columns 3'"
  mod4 + shift + P

"i3lock -u -d -c 000000"
  mod4 + z

"pkill -USR1 redshift"
  mod4 + d
